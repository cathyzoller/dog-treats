{
  "name": "dog-treats",
  "version": "0.3.2",
  "description": "Give your dog a steak",
  "main": "src/server",
  "engines": {
    "node": "6.5.0",
    "npm": "3.10.3"
  },
  "scripts": {
    "test": "test",
    "clean": "rm -rf ./build",
    "lint": "eslint --fix --ext js --ext jsx src",
    "build-langs": "./node_modules/.bin/babel-node src/utils/translator.js",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d ./build/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node ./build/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run -- ./src/server",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cathyzoller/dog-treats.git"
  },
  "keywords": [
    "apollo-client",
    "graphql",
    "react",
    "redux"
  ],
  "author": "Cathy Zoller",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cathyzoller/dog-treats/issues"
  },
  "homepage": "https://github.com/cathyzoller/dog-treats/#readme",
  "dependencies": {
    "aphrodite": "^0.4.1",
    "apollo-client": "^0.3.29",
    "apollo-server": "^0.3.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "fs": "^0.0.1-security",
    "graphql": "^0.7.2",
    "graphql-tag": "^0.1.9",
    "graphql-tools": "^0.7.2",
    "json-loader": "^0.5.4",
    "minilog": "^3.0.1",
    "pg": "^6.1.0",
    "react": "^15.2.1",
    "react-apollo": "^0.3.15",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.2.1",
    "react-intl": "^2.1.5",
    "react-intl-redux": "^0.1.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "rollbar": "^0.6.2",
    "sequelize": "^3.24.3",
    "thinky": "^2.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-register": "^6.9.0",
    "casual": "^1.5.3",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "foreman": "^1.4.1",
    "nodemon": "^1.9.2",
    "webpack-dev-server": "^1.14.1"
  }
}
